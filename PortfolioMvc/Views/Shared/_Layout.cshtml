                    <a class="nav-link text-white" asp-area="" asp-controller="Portfolio" asp-action="Index"><h4>Portfólio</h4></a>
                    <a class="nav-link text-white" asp-area="" asp-controller="Portfolio" asp-action="Projetos"><h4>Projetos</h4></a>
                    <a class="nav-link text-white" asp-area="" asp-controller="Portfolio" asp-action="Contato"><h4>Contato</h4></a>
                    @* <a href="https://www.instagram.com/legiaokmusic/" target="_blank" id="redesocial"><h5 class="bi bi-instagram right-icons "></h5></a> *@
